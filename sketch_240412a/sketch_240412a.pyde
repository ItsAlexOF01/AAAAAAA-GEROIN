def setup():
    size(900,900)
    background(255,255,255)



def draw():
    if keyPressed:
        if key == 'b' or key == 'B':
            rect(random(900),random(900),50,50)
            fill(random(255),random(255),random(255))
        if key == 'q' or key == 'Q':
            background(255,255,255)
         
        
           
        
        
    
